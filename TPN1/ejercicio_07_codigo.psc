Algoritmo ejercicio_07_codigo
	// Ingresar un importe en pesos y mostrar el 80 MOD , 20 MOD  y 15  MOD  de dicho valor.
	Escribir 'Ingresar importe en pesos'
	Leer importe
	porcentaje <- importe*0.8
	Escribir 'El 80%: ', porcentaje
	porcentaje <- importe*0.2
	Escribir 'El 20%: ', porcentaje
	porcentaje <- importe*0.15
	Escribir 'El 15%: ', porcentaje
FinAlgoritmo
